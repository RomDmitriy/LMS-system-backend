{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "xo",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "new-cap": "off",
        "object-curly-spacing": "off",
        "indent": ["error", 4],
        "capitalized-comments": "off",
        "radix": "off",
        "camelcase": "off"
    }
}
